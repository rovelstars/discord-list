{
  "name": "rovel-discord-list",
  "description": "Rovel Discord List",
  "version": "1.0.0",
  "scripts": {
    "start": "run-p css-compile main",
    "main": "node src/index.js",
    "server": "node src/server/app.js",
    "css-compile": "node-sass src/sass/ -r  -o src/public/assets/css/ --output-style compressed"
  },
  "engines": {
    "node": "^15.x"
  },
  "dependencies": {
    "response-time": "2.3.2",
    "validator": "13.5.2",
    "sanitize-html": "2.3.3",
    "node-schedule": "2.0.0",
    "helmet": "4.4.1",
    "cookie-parser": "1.4.5",
    "disco-oauth": "5.0.1",
    "express-rate-limit": "5.2.6",
    "compression": "1.7.4",
    "mongodb": "3.6.4",
    "mongoose": "5.11.15",
    "express": "4.17.1",
    "ejs": "3.1.6",
    "rovel.js": "1.2.0",
    "discord.js": "12.5.3",
    "express-session": "1.17.1",
    "express-fileupload": "1.2.1",
    "passport": "0.4.1",
    "marked": "2.0.1",
    "@sentry/node": "6.2.1",
    "@sentry/tracing": "6.2.2",
    "module-alias": "2.2.2",
    "@meanie/mongoose-to-json": "2.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.3",
    "postcss-cli": "^8.3.1",
    "bulma": "0.9.2"
  },
  "_moduleAliases": {
    "@root": "src",
    "@utils": "src/utils",
    "@routes": "src/server/routes",
    "@models": "src/models",
    "@views": "src/views",
    "@assets": "src/public/assets",
    "@bot": "src/bot",
    "@sdk": "src/sdk-nodejs",
    "@server": "src/server"
  }
}
