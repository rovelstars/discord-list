{
  "name": "rovel-discord-list",
  "description": "Rovel Discord List",
  "version": "0.0.3",
  "scripts": {
    "start": "node src/index.js",
    "build": "node build.js",
    "build-f": "node build.js --force",
    "all": "npm run build-f && npm start",
    "pretty": "npx prettier --write \"./**/*.{js,jsx,json}\""
  },
  "cacheDirectories": [
    "node_modules",
    "src/public"
  ],
  "engines": {
    "node": "^16.x"
  },
  "checkCache": "true",
  "dependencies": {
    "@sentry/node": "6.17.2",
    "@sentry/tracing": "^6.9.0",
    "cloudflare-express": "1.0.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "diacritics": "1.3.0",
    "disco-oauth": "github:disco-oauth/disco-oauth",
    "discord.js": "13.8.0",
    "ejs": "3.1.6",
    "express": "^4.17.1",
    "express-actuator": "1.7.1",
    "express-fileupload": "1.2.1",
    "express-rate-limit": "^5.3.0",
    "express-session": "^1.17.2",
    "express-ws": "^5.0.2",
    "fetch-base64": "2.1.2",
    "helmet": "4.6.0",
    "html-entities": "2.3.2",
    "html-minifier": "4.0.0",
    "i18n": "0.13.3",
    "isomorphic-ws": "4.0.1",
    "marked": "^4.0.12",
    "min-indent": "1.0.1",
    "module-alias": "^2.2.2",
    "mongodb": "4.0.1",
    "mongoose": "7.4.0",
    "node-fetch": "2",
    "node-schedule": "2.0.0",
    "passport": "0.4.1",
    "proxy-list-random": "1.0.2",
    "purgecss": "4.0.3",
    "rdl-i18n": "github:rovelstars/rdl-i18n",
    "rovel.js": "1.2.0",
    "sanitize-html": "2.4.0",
    "shelljs": "0.8.4",
    "socket.io": "^4.2.0",
    "translatte": "^3.0.1",
    "validator": "13.7.0",
    "ws": "^7.5.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "bulma": "0.9.3",
    "ejs-lint": "^1.2.1",
    "eslint": "^7.31.0",
    "keyboard-css": "1.2.4",
    "node-sass": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "pm2": "5.1.0",
    "postcss": "^8.3.11",
    "postcss-cli": "latest"
  },
  "_moduleAliases": {
    "@root": "src",
    "@utils": "src/utils",
    "@routes": "src/server/routes",
    "@mw": "src/server/mw",
    "@models": "src/models",
    "@views": "src/views",
    "@assets": "src/public/assets",
    "@bot": "src/bot",
    "@sdk": "src/sdk-nodejs",
    "@server": "src/server"
  }
}
