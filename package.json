{
  "name": "RDL",
  "description": "Rovel Discord List",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm i npm@latest -g && npm i rovelstars/rovel.js#master -g",
    "start": "npm run main",
    "main": "node src/index.js",
    "server": "node src/server/app.js"
  },
  "engines": {
    "node": "^14.x"
  },
  "dependencies": {
    "express-rate-limit": "5.2.5",
    "compression": "1.7.4",
    "mongodb": "3.6.4",
    "mongoose": "5.11.17",
    "express": "4.17.1",
    "ejs": "3.1.6",
    "rovel.js": "1.1.9",
    "discord.js": "12.5.1",
    "express-session": "1.17.1",
    "express-fileupload": "1.2.1",
    "passport": "0.4.1",
    "marked": "2.0.0",
    "@sentry/node": "6.1.0",
    "@sentry/tracing": "6.0.1",
    "module-alias": "2.2.2",
    "body-parser": "1.19.0",
    "@meanie/mongoose-to-json": "2.5.0"
  },
  "_moduleAliases": {
    "@root": "src",
    "@utils": "src/utils",
    "@routes": "src/server/routes",
    "@models": "src/models",
    "@views": "src/views",
    "@assets": "src/public/assets",
    "@bot": "src/bot",
    "@server": "src/server"
  }
}
